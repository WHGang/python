# …or create a new repository on the command line
echo "# python" >> README.md
git init

git add .
git commit -m "update"
git branch -M main
git remote add python https://github.com/WHGang/python.git
git push -u python main




# …or push an existing repository from the command line
git add .
git commit -m "update"
git branch -M main
git push -u python main




git push -f origin master强制push



